{
  "expo": {
    "name": "Puntos Ciudadanos",
    "slug": "puntos-ciudadanos",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "scheme": "puntosciudadanos",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#2c5282"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.puntosciudadanos.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#2c5282"
      },
      "package": "com.puntosciudadanos.app"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
